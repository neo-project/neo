<!-- Root element for configuration settings -->
<Configuration>
    <!-- Application-specific configuration settings -->
    <ApplicationConfiguration>
        <!-- Logger configuration -->
        <Logger>
            <Path>Logs</Path> <!-- Directory for storing log files -->
            <ConsoleOutput>false</ConsoleOutput> <!-- Indicates whether console output is enabled (false = disabled) -->
            <Active>false</Active> <!-- Indicates whether logging is active (false = inactive) -->
        </Logger>
        <!-- Storage configuration -->
        <Storage>
            <Engine>LevelDBStore</Engine> <!-- Type of storage engine used, candidates ["LevelDBStore", "RockDBStore", "MemoryStore"] -->
            <Path>Data_LevelDB_{0}</Path> <!-- Path for the storage data, {0} is a placeholder -->
        </Storage>
        <!-- P2P network configuration -->
        <P2P>
            <Port>20333</Port> <!-- Port number for P2P connections -->
            <MinDesiredConnections>10</MinDesiredConnections> <!-- Minimum number of desired connections -->
            <MaxConnections>40</MaxConnections> <!-- Maximum number of connections allowed -->
            <MaxConnectionsPerAddress>3</MaxConnectionsPerAddress> <!-- Maximum connections allowed per address -->
        </P2P>
        <!-- Wallet unlocking configuration -->
        <UnlockWallet>
            <Path></Path> <!-- Path to the wallet file -->
            <Password></Password> <!-- Wallet password -->
            <IsActive>false</IsActive> <!-- Indicates if wallet unlocking is active (false = inactive) -->
        </UnlockWallet>
        <!-- Contracts configuration -->
        <Contracts>
            <NeoNameService>0x50ac1c37690cc2cfc594472833cf57505d5f46de</NeoNameService> <!-- Specific contract address -->
        </Contracts>
    </ApplicationConfiguration>

    <!-- Protocol-specific configuration settings -->
