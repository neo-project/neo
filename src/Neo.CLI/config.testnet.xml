        <AddressVersion>53</AddressVersion> <!-- Version of the address protocol -->
        <MillisecondsPerBlock>15000</MillisecondsPerBlock> <!-- Duration of each block in milliseconds -->
        <MaxTransactionsPerBlock>5000</MaxTransactionsPerBlock> <!-- Maximum number of transactions per block -->
        <MemoryPoolMaxTransactions>50000</MemoryPoolMaxTransactions> <!-- Maximum number of transactions in the memory pool -->
        <MaxTraceableBlocks>2102400</MaxTraceableBlocks> <!-- Maximum number of blocks that can be traced -->
        <!-- Hardfork configuration -->
