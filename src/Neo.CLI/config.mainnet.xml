<Configuration>
    <!-- Application configuration settings -->
    <ApplicationConfiguration>
        <!-- Logger configuration -->
        <Logger>
            <Path>Logs</Path> <!-- Directory for log files -->
            <ConsoleOutput>false</ConsoleOutput> <!-- Toggle for console output -->
            <Active>false</Active> <!-- Toggle to activate/deactivate logging -->
        </Logger>
        <!-- Storage configuration -->
        <Storage>
            <Engine>LevelDBStore</Engine> <!-- Type of storage engine used, candidates ["LevelDBStore", "RockDBStore", "MemoryStore"] -->
            <Path>Data_LevelDB_{0}</Path> <!-- Path for storage data, {0} is a placeholder for the network id -->
        </Storage>
        <!-- P2P network settings -->
        <P2P>
            <Port>10333</Port> <!-- Port number for P2P connections -->
            <MinDesiredConnections>10</MinDesiredConnections> <!-- Minimum desired connections -->
            <MaxConnections>40</MaxConnections> <!-- Maximum number of connections -->
            <MaxConnectionsPerAddress>3</MaxConnectionsPerAddress> <!-- Maximum connections per address -->
        </P2P>
        <!-- Wallet unlock settings -->
        <UnlockWallet>
            <Path></Path> <!-- Path to the wallet file -->
            <Password></Password> <!-- Password for the wallet -->
            <IsActive>false</IsActive> <!-- Toggle to activate/deactivate wallet unlocking -->
        </UnlockWallet>
        <!-- Contracts settings -->
        <Contracts>
            <NeoNameService>0x50ac1c37690cc2cfc594472833cf57505d5f46de</NeoNameService> <!-- Specific contract address -->
        </Contracts>
    </ApplicationConfiguration>

